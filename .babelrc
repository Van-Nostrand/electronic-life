{
  "presets": [
    "@babel/preset-env",
    "@babel/preset-react"
  ],
  "plugins": [
    "@babel/plugin-transform-arrow-functions",
    "@babel/plugin-proposal-class-properties",
    [
      "module-resolver",
      {
        "root": "./src",
        "extensions": [".js", ".ts", ".tsx", ".scss", ".css", ".json", ".svg"],
        "alias": {
          "@": "./src"
        }
      }
    ]
  ]
}
